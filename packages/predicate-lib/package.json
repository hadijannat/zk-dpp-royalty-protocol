{
  "name": "@zkdpp/predicate-lib",
  "version": "0.1.0",
  "description": "Predicate registry and metadata for ZK-DPP circuits",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "clean": "rm -rf dist",
    "generate": "tsx src/generate-battery-predicates.ts"
  },
  "files": [
    "dist",
    "predicates.json"
  ],
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.17"
  }
}
